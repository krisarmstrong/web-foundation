{
  "name": "@krisarmstrong/web-foundation",
  "version": "0.3.2",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./tokens": "./src/tokens.ts"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts src/tokens.ts --dts --sourcemap --format esm,cjs --out-dir dist",
    "dev": "tsup src/index.ts src/tokens.ts --dts --watch --format esm,cjs --out-dir dist",
    "lint": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.9.3"
  },
  "license": "MIT"
}
