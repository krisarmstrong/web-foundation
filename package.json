{
  "name": "@krisarmstrong/web-foundation",
  "version": "0.9.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tokens": {
      "types": "./dist/tokens.d.ts",
      "import": "./dist/tokens.js",
      "require": "./dist/tokens.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts src/tokens.ts --dts --sourcemap --format esm,cjs --out-dir dist",
    "dev": "tsup src/index.ts src/tokens.ts --dts --watch --format esm,cjs --out-dir dist",
    "lint": "tsc --noEmit",
    "prepare": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@sentry/react": "^10.25.0",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "rimraf": "^6.0.1",
    "storybook": "^10.0.7",
    "tsup": "^8.3.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9",
    "playwright": "^1.56.1",
    "@vitest/browser-playwright": "^4.0.9",
    "@vitest/coverage-v8": "^4.0.9",
    "@tailwindcss/postcss": "^4.1.17",
    "postcss": "^8.5.6",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/addon-onboarding": "^10.0.7",
    "@storybook/addon-vitest": "^10.0.7"
  },
  "license": "MIT"
}
